        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (newValue === '') {
        return;
    }
    // If user selects legal request type, add new option to What needed field
    var reqType = g_form.getValue('u_requested_type');
	var whatNeeded = g_form.getValue('u_what_needed');
	
    if (newValue == 'legal') {
        g_form.addOption('u_what_needed', 'u_test', 'test');
    }
	
    if(isLoading && !g_form.isNewRecord()){
      g_form.setValue('u_what_needed', whatneeded);
    }
}]]></script>
